            <Label layoutX="33.0" layoutY="51.0" text="Choisissez votre date" />
            <ComboBox fx:id="cboenseignant" disable="true" layoutX="14.0" layoutY="257.0" prefWidth="150.0" />
            <ComboBox fx:id="cboforfait" layoutX="14.0" layoutY="153.0" prefWidth="150.0" />
            <Button layoutX="195.0" layoutY="153.0" mnemonicParsing="false" onMouseClicked="#onRechercherClicked" text="Rechercher" />
            <DatePicker fx:id="datereserve" layoutX="12.0" layoutY="76.0" onAction="#dateclicked" />
            <Button fx:id="btnreserve" disable="true" layoutX="14.0" layoutY="301.0" mnemonicParsing="false" onMouseClicked="#ReservedClicked" text="Réservé" />
