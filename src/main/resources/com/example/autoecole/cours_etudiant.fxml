<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.autoecole.CoursEtudiant">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
